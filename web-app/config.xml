

<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.phonegap.example"
        versionCode="10"
        version   = "1.0.0">

    <!-- versionCode is optional and Android only -->

    <name>ThreeCircles</name>

    <description>
    </description>

    <author href="https://build.phonegap.com" email="support@phonegap.com">
    3musket33rs
    </author>

    <access origin="*" subdomains="true" />
    <preference name="load-url-timeout" value="60000"/>
    <preference name="permissions" value="android.permission.ACCESS_COARSE_LOCATION" />
    <preference name="permissions" value="android.permission.ACCESS_FINE_LOCATION" />
    <preference name="permissions" value="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
    <plugins>
        <plugin name="Geolocation" value= "org.apache.cordova.GeoBroker"/>
    </plugins>
</widget>